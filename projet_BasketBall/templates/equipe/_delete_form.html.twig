<form method="post" action="{{ path('equipe_delete', {'id_equipe': equipe.getIdEquipe}) }}" onsubmit="return confirm('Vous êtes sûr de vouloir la supprimer ?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ equipe.getIdEquipe) }}">
    <button class="button is-danger">Supprimer</button>
</form>
