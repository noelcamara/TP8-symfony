{% extends 'base.html.twig' %}

{% block title %}Matchs{% endblock %}

{% block content %}
    <h1>Matchs</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id_match</th>
                <td>{{ match.idMatch }}</td>
            </tr>
            <tr>
                <th>Date_match</th>
                <td>{{ match.dateMatch }}</td>
            </tr>
            <tr>
                <th>Elim_directe</th>
                <td>{{ match.elimDirecte }}</td>
            </tr>
            <tr>
                <th>Type</th>
                <td>{{ match.type }}</td>
            </tr>
            <tr>
                <th>Score_equipe1</th>
                <td>{{ match.scoreEquipe1 }}</td>
            </tr>
            <tr>
                <th>Score_equipe2</th>
                <td>{{ match.scoreEquipe2 }}</td>
            </tr>
            <tr>
                <th>Id_equipe1</th>
                <td>{{ match.idEquipe1 }}</td>
            </tr>
            <tr>
                <th>Id_equipe2</th>
                <td>{{ match.idEquipe2 }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('matchs_index') }}">back to list</a>

    <a href="{{ path('matchs_edit', {'id_match': match.getIdMatch}) }}">edit</a>

    {{ include('matchs/_delete_form.html.twig') }}
{% endblock %}
